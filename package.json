{
  "name": "comment-pull-request",
  "version": "2.4.3",
  "description": "GitHub action for commenting pull-request",
  "main": "lib/main.js",
  "scripts": {
    "build": "npm run build:main && npm run build:cleanup",
    "build:main": "ncc build src/main.ts -o lib",
    "build:cleanup": "ncc build src/cleanup.ts -o lib/cleanup",
    "lint": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pass-culture-github-actions/comment-pull-request"
  },
  "keywords": [
    "actions",
    "container",
    "pull-request",
    "comment",
    "message"
  ],
  "author": "Dimitri Kopriwa <dimitri.kopriwa@passculture.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pass-culture-github-actions/comment-pull-request/issues"
  },
  "homepage": "https://github.com/pass-culture-github-actions/comment-pull-request#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.0.0",
    "@actions/github": "^5.0.3",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "@types/node": "^18.16.2",
    "@vercel/ncc": "^0.38.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
